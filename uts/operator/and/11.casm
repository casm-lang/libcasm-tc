//@ TC()

// | AND   | undef | false | true  | sym   |
// |-------+-------+-------+-------+-------|
// | undef | undef | false | undef | sym'  |
// | false | false | false | false | false |
// | true  | undef | false | true  | sym'  |
// | sym   | sym'  | false | sym'  | sym'  |

CASM unittest init foo

rule foo =
{
    let a = false in
    let b = false in
    let c = a and b in
    {
        assert c  = false
        
        assert c != undef
        assert c != true
        assert symbolic( c ) = false
    }
    program( self ) := undef
}
